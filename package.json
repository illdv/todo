    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.8",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
